				["LAI|LAI-max", "MAX"],
				["Yield|Yield-last", "LAST"],
				["Count|Cycle-length", "SUM"],
				["TraDef|TraDef-avg", "AVG"],
				["NDef|NDef-avg", "AVG"]
			]
		],


