				["Tavg|Tavg5", "AVG"],
				["Precip|Precip5", "SUM"],
				["LAI|LAI5", "AVG"],
				["AbBiom|AbBiom5", "AVG"],
				["guenther-isoprene-emission|G-iso5", "SUM"],
				["guenther-monoterpene-emission|G-mono5", "SUM"],
