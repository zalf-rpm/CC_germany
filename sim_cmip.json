				["CM-count", "FIRST"],
				["Year", "LAST"],
				["Yield|yield", "LAST"],
				["LAI|crop-max-LAI", "MAX"],
				["AbBiom|biom-final", "LAST"],
				["DOY|sowing-doy", "FIRST"]
			],

			"maturity", [
				"CM-count",
				"DOY|maturity-doy"
			],

			"anthesis", [
				"CM-count",
				"DOY|anthesis-doy"
			],

			"Harvest", [
				"CM-count",
				"DOY|harvest-doy"
			]
		],

		"full events": [
			"crop", [
				["Crop", "FIRST"],
				["CM-count", "FIRST"],
				["Year", "LAST"],
				["Yield|yield", "LAST"],
				["Precip|crop-sum-precip", "SUM"],
				["LAI|crop-max-LAI", "MAX"],
				["TraDef|crop-avg-transpiration-deficit", "AVG"],
				["Tavg|crop-avg-tavg", "AVG"],
				["NFert|crop-sum-nfert", "SUM"]
